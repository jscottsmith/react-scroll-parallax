{
  "version": "1.3.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jscottsmith/parallax-controller"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "np"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "parallax-controller",
  "description": "Core classes and controller for creating parallax scrolling effects",
  "keywords": [
    "scroll",
    "effects",
    "parallax",
    "animation",
    "transform",
    "translate",
    "rotate",
    "scale",
    "opacity"
  ],
  "author": "J Scott Smith",
  "module": "dist/parallax-controller.esm.js",
  "size-limit": [
    {
      "path": "dist/parallax-controller.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/parallax-controller.esm.js",
      "limit": "10 KB"
    }
  ],
  "jest": {
    "setupFiles": [
      "./src/setupTests.ts"
    ]
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.3",
    "husky": "^7.0.4",
    "np": "^7.6.0",
    "size-limit": "^7.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "bezier-easing": "^2.1.0"
  }
}
