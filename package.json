{
    "name": "react-scroll-parallax",
    "version": "1.0.0-beta.0",
    "description": "React component to create parallax scrolling effects",
    "repository": {
        "type": "git",
        "url": "https://github.com/jscottsmith/react-scroll-parallax"
    },
    "main": "lib/index.js",
    "scripts": {
        "dev": "yarn test && webpack --progress --colors --watch",
        "dev-server": "node ./example/dist/server",
        "gh-pages": "NODE_ENV=production webpack --progress --colors",
        "test": "BABEL_ENV=test jest && yarn run storybook:export",
        "test:watch": "BABEL_ENV=test jest --watch",
        "prettier":
            "prettier --tab-width 4 --single-quote --trailing-comma es5 --print-width 80 --write \"{src,examples,__tests__}/**/**/**/*.js\"",
        "prepublish":
            "babel ./src --out-dir ./lib --presets es2015,react,stage-0 --plugins babel-plugin-add-module-exports",
        "storybook": "BABEL_ENV=dev start-storybook -p 6006",
        "storybook:export":
            "BABEL_ENV=dev build-storybook -c .storybook -o build",
        "build-storybook": "build-storybook",
        "deploy": "yarn run storybook:export"
    },
    "jest": {
        "modulePaths": ["<rootDir>/src/"]
    },
    "keywords": ["react", "parallax", "scroll", "component"],
    "author": "J Scott Smith",
    "license": "ISC",
    "dependencies": {
        "prop-types": "^15.5.10",
        "react": "^15.0.0",
        "react-dom": "^15.0.0",
        "react-observed": "^1.0.0-beta.2"
    },
    "devDependencies": {
        "@storybook/cli": "^3.2.15",
        "@storybook/addon-actions": "^3.2.15",
        "@storybook/addon-knobs": "^3.2.15",
        "@storybook/addon-links": "^3.2.15",
        "@storybook/react": "^3.2.15",
        "babel-cli": "^6.24.1",
        "babel-core": "^6.23.1",
        "babel-jest": "^20.0.3",
        "babel-loader": "^6.3.2",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-module-resolver": "^2.7.1",
        "babel-preset-env": "^1.1.8",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.23.0",
        "babel-preset-stage-0": "^6.24.1",
        "cross-env": "^3.1.4",
        "css-loader": "^0.28.0",
        "express": "^4.14.1",
        "intersection-observer": "^0.4.2",
        "jest": "^20.0.4",
        "node-sass": "^4.5.0",
        "postcss-loader": "^1.3.3",
        "react-test-renderer": "^15.6.1",
        "regenerator-runtime": "^0.10.5",
        "sass-loader": "^6.0.3",
        "style-loader": "^0.16.1",
        "webpack": "^2.2.1",
        "webpack-merge": "^4.1.0",
        "webpack-node-externals": "^1.5.4"
    }
}
