{
    "name": "react-scroll-parallax",
    "version": "2.0.0-alpha.2",
    "description": "React component to create parallax scrolling effects",
    "repository": {
        "type": "git",
        "url": "https://github.com/jscottsmith/react-scroll-parallax"
    },
    "main": "lib/index.js",
    "scripts": {
        "dev": "yarn test && cross-env BABEL_ENV=dev webpack --progress --colors --watch",
        "dev-server": "node ./example/dist/server",
        "gh-pages": "cross-env NODE_ENV=production webpack --progress --colors",
        "test": "cross-env BABEL_ENV=test jest",
        "test:watch": "cross-env BABEL_ENV=test jest --watch",
        "test:coverage": "cross-env BABEL_ENV=test jest && codecov",
        "prettier": "prettier --tab-width 4 --single-quote --trailing-comma es5 --print-width 80 --write \"{src,examples,__tests__}/**/**/**/*.js\"",
        "prepublish": "babel ./src --out-dir ./lib --presets es2015,react,stage-0 --plugins babel-plugin-add-module-exports",
        "storybook": "cross-env BABEL_ENV=dev start-storybook -p 6006",
        "storybook:export": "cross-env BABEL_ENV=dev build-storybook -c .storybook -o build",
        "build-storybook": "build-storybook",
        "deploy": "yarn run storybook:export"
    },
    "jest": {
        "modulePaths": [
            "<rootDir>/src/"
        ],
        "coverageDirectory": "./coverage/",
        "collectCoverage": true
    },
    "keywords": [
        "react",
        "parallax",
        "scroll",
        "component"
    ],
    "author": "J Scott Smith",
    "license": "ISC",
    "peerDependencies": {
        "react": "^15.0.0 || ^16.0.0"
    },
    "dependencies": {
        "prop-types": "^15.5.7",
        "react-observed": "^1.0.0-beta.4"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^3.2.17",
        "@storybook/addon-knobs": "^3.2.17",
        "@storybook/addon-links": "^3.2.17",
        "@storybook/cli": "^3.2.17",
        "@storybook/react": "^3.2.17",
        "babel-cli": "^6.24.1",
        "babel-core": "^6.23.1",
        "babel-jest": "^20.0.3",
        "babel-loader": "^6.3.2",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-module-resolver": "^2.7.1",
        "babel-preset-env": "^1.1.8",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "codecov": "^3.0.0",
        "cross-env": "^3.2.4",
        "css-loader": "^0.28.0",
        "express": "^4.14.1",
        "intersection-observer": "^0.4.2",
        "jest": "^21.3.0-beta.2",
        "node-sass": "^4.5.0",
        "postcss-loader": "^1.3.3",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-test-renderer": "^16.2.0",
        "regenerator-runtime": "^0.10.5",
        "sass-loader": "^6.0.3",
        "style-loader": "^0.16.1",
        "webpack": "^2.2.1",
        "webpack-merge": "^4.1.0",
        "webpack-node-externals": "^1.5.4"
    }
}
