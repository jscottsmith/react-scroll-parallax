{
	"rules": {
		"array-callback-return": 0,
		"func-name-matching": 0,
		"max-lines-per-function": 0,
		"max-statements-per-line": [2, { "max": 2 }],
		"no-restricted-properties": 1,
		"symbol-description": 0,
		"prefer-promise-reject-errors": 0,
		"consistent-return": 0,
	},
	"env": {
		"browser": true,
		"mocha": true,
	},
	"globals": {
		"Symbol": false,
		"Promise": false,
		"assert": false,
		"expect": false,
		"chai": false,
	}
}
